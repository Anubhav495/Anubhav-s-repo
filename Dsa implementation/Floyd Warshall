#pragma GCC optimize ("O3")
#pragma GCC target ("sse4")
#include <bits/stdc++.h>
using namespace std;
#define FAST  ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0)
#define ll long long int 
#define ar array

const int mxn=500;
int dist[mxn][mxn];
int adj[mxn][mxn];

int main()
{
	FAST;
	//#ifndef ONLINE_JUDGE
	//	freopen("input.txt","r",stdin);
	//	freopen("output.txt","w",stdout);
	//#endif
	ll testcase;
	cin>>testcase;
	for(ll t=0;t<testcase;t++)
	{
		int n,m;
		cin>>n>>m;
		for(int i=0;i<m;i++)
		{
			int x,y,weight;
			cin>>x>>y>>weight;
			--x,--y;
			adj[x][y]=weight;
		}
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(i==j)
				{
					dist[i][j]=0;
				}
				else if(adj[i][j])
				{
					dist[i][j]=adj[i][j];
				}
				else
				{
					dist[i][j]=INT_MAX;
				}
			}
		}
		for(int k=0;k<n;k++)
		{
			for(int i=0;i<n;i++)
			{
				for(int j=0;j<n;j++)
				{
					dist[i][j]=min({dist[i][j],dist[i][k]+dist[k][j]});
				}
			}
		}
		
		
	}
		
	return 0;
}


























