	{
		int n;
		cin>>n;
		int a[n-1];
		for(int i=0;i<n-1;i++)
		{
			cin>>a[i];
		}
		sort(a,a+n-1);
		for(int i=0;i<n-1;i++)
		{
			if(a[i]!=i+1)
			{
				cout<<i+1<<' ';
				return 0;
			}
		}
    cout<<n<<' ';
	}
		
}
